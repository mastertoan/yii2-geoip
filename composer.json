{
  "name": "mastertoan/yii2-geoip",
  "description": "Yii 2 GeoIP extension. Returns country, city, lat, lng of current or specified IP (uses MaxMind's GeoIP2 databases)",
  "keywords": ["geoip", "geolocation", "maxmind", "yii2"],
  "type": "yii2-extension",
  "version": "1.0.4",
  "license": "MIT",
  "authors": [
    {
      "name": "Bohdan Lysenko",
      "email": "mastertoan2011@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.0",
    "maxmind-db/reader": "~1.0",
    "mastertoan/maxmind-geolite2-database": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "mastertoan\\GeoIP\\": "src/"
    }
  }
}
